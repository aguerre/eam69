{% extends 'EAMDefaultBundle::layout.html.twig' %}
{% block body %}
<div class="wrapper">
    {% include 'EAMDefaultBundle::header.html.twig' %}
    <div class="clear"></div>
    <!--- Banner Start -->
    <div class="banner">
        <div class="flexslider">
            <ul class="slides">
                <li>
                    {% image '@EAMDefaultBundle/Resources/public/images/banner.png' %}
                    <img src="{{ asset_url }}" alt="" />
                    {% endimage %}
                </li>
            </ul>
        </div>
    </div>
    <!--- Banner End -->
    <!-- Player Start -->
    <div class="plyer-sec webkit">
    {# <!-- Audio Plyer Strat -->
     <div class="audio-plyer webkit">
            <div class="container">
                <div id="player">
                    <div id="jquery_jplayer_n" class="jp-jplayer"></div>
                    <div id="jp_container_n" class="jp-audio">
                        <div class="jp-type-playlist">
                            <div class="jp-gui">
                                <div class="jp-interface">
                                    <div class="jp-controls-holder">
                                        <ul class="jp-controls audio-control">
                                            <li>
                                                <a href="javascript:;" class="jp-previous color" tabindex="1"> <em class="fa fa-backward"></em>
                                                </a>
                                            </li>
                                            <li>
                                                <a href="javascript:;" class="jp-play color" tabindex="1"> <em class="fa fa-play"></em>
                                                </a>
                                            </li>
                                            <li>
                                                <a href="javascript:;" class="jp-pause color" tabindex="1">
                                                    <em class="fa fa-pause"></em>
                                                </a>
                                            </li>
                                            <li>
                                                <a href="javascript:;" class="jp-next color" tabindex="1">
                                                    <em class="fa fa-forward"></em>
                                                </a>
                                            </li>
                                        </ul>
                                        <div class="main-progress">
                                            <div class="jp-progress">
                                                <div class="jp-seek-bar">
                                                    <div class="jp-play-bar bgcolor"></div>
                                                </div>
                                            </div>
                                            <div class="jp-current-time"></div>
                                            <div class="jp-duration"></div>
                                            <div class="volume-wrap">
                                                <a title="mute" tabindex="1" class="jp-mute" href="javascript:;"><em class="fa fa-volume-down"></em></a>
                                                <a title="unmute" tabindex="1" class="jp-unmute" href="javascript:;" style="display: none;"><em class="fa fa-volume-off"></em></a>
                                                <div class="jp-volume-bar">
                                                    <div class="jp-volume-bar-value bgcolor" style="width: 80%;"></div>
                                                </div>
                                                <a title="max volume" tabindex="1" class="jp-volume-max" href="javascript:;"><em class="fa fa-volume-up"></em></a>
                                            </div>
                                            <a href="#playheader" class="jp-playlist-icon btntoggle webkit"><em class="fa fa-headphones fa-2x"></em><span class="color">CRAS LOBORTIS AUGUE-JHON DOE</span></a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="jp-playlist " id="playheader">
                                <div class="wrapper-payerlsit">
                                <ul>
                                    <!-- The method Playlist.displayPlaylist() uses this unordered list -->                    
                                    <li></li>
                                </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
   <!-- Audio Plyer End -->
    </div> #}
    <!-- Player End -->
    <div class="clear"></div>
    <!-- Content Start -->
    <div class="content">
        <!-- Container Start -->
        <div class="container">
            <!-- Row Start -->
            <div class="row">
                <!-- Span12 Start -->
                <div class="span12">
                    <!-- Featured Start -->
                    <div class="featured-artist">
                        <div class="heading"><h2><span class="color">Saison</span> {{ ''|date('Y') }}</h2></div>
                        <div class="minus-margin">
                            <article>
                                <figure>
                                    <a href="#"><i class="fa fa-camera"></i></a>
                                    <figcaption>
                                        <a href="{{ path('eam_default_default_albumsinannee', {'annee': 'now'|date('Y') }) }}"></a>
                                    </figcaption>
                                </figure>
                                <div class="user-text">
                                    <div class="sec">
                                        <h4><a href="{{ path('eam_default_default_albumsinannee', {'annee': 'now'|date('Y') }) }}">Photos</a></h4>
                                        <p></p>
                                    </div>
                                </div>
                            </article>
                            <article>
                                <figure>
                                    <a href="{{ path('eam_default_default_resultats') }}"><i class="fa fa-university"></i></a>
                                    <figcaption>
                                        <a href="{{ path('eam_default_default_resultats') }}"></a>
                                    </figcaption>
                                </figure>
                                <div class="user-text">
                                    <div class="sec">
                                        <h4><a href="{{ path('eam_default_default_resultats') }}">Résultats</a></h4>
                                        <p></p>
                                    </div>
                                </div>
                            </article>
                            <article>
                                <figure>
                                    <a href="{{ path('eam_default_default_palmares') }}"><i class="fa fa-star"></i></a>
                                    <figcaption>
                                        <a href="{{ path('eam_default_default_palmares') }}"></a>
                                    </figcaption>
                                </figure>
                                <div class="user-text">
                                    <div class="sec">
                                        <h4><a href="{{ path('eam_default_default_palmares') }}">Palmarès</a></h4>
                                        <p></p>
                                    </div>
                                </div>
                            </article>
                            <article>
                                <figure>
                                    <a href="{{ path('eam_default_default_records') }}"><i class="fa fa-trophy"></i></a>
                                    <figcaption>
                                        <a href="{{ path('eam_default_default_records') }}"></a>
                                    </figcaption>
                                </figure>
                                <div class="user-text">
                                    <div class="sec">
                                        <h4><a href="{{ path('eam_default_default_records') }}">Records</a></h4>
                                        <p></p>
                                    </div>
                                </div>
                            </article>
                        </div>
                    </div>
                    <!-- Featured End -->
                </div>
                <!-- Span12 End -->
                <!-- Span4 Start -->
                <div class="span4">
                    {% if article is defined and article is not empty %}
                    <div class="heading"><h2><span class="color">Dernier </span> article</h2></div>
                    <div class="blog-v2 webkit slide-view">
                        <article class="featured">
                            <figure>
                                <div class="flexslider">
                                    <ul class="slides">
                                        <li>
                                            <div class="clear"></div>
                                            <div class="text">
                                                <h3><a href="blog-detail.html" class="colorhover">{{ article.titre }}</a></h3>
                                                <p>{{ article.contenu }}</p>
                                                <!-- <a class="bgcolor nr-readmore" href="">Lire l'article</a> -->
                                            </div>
                                        </li>
                                    </ul>
                                </div>
                            <figcaption class="bgcolor">20 <span>May</span></figcaption>
                            </figure>
                        </article>
                    </div>
                    {% endif %}
                </div>
                <!-- Span4 Start -->
                <!-- Span12 Start -->
                <div class="span12">
                    <!-- Upcooming Event Start -->
                    <div class="upcoming-event">
                        <div class="heading"><h2><span class="color">Événement</span> à venir</h2></div>
                        <div class="minus-margin">
                            <article>
                                <div class="top-text">
                                    <h3>Melbourne City Hall</h3>
                                    <p>Lorem ipsum dolor sit amet, consetetur sadipscing elitr, sed diam nonumy eirmod tempor invidunt ut labore et dolore magna aliquyam erat, sed diam voluptua. At vero eos et</p>
                                    <a class="bgcolor" href="event-detail.html">More</a>
                                </div>
                                <div class="bottom-text">
                                    <figure><a href="event-detail.html"><img src="images/event1.jpg" alt=""></a></figure>
                                    <div class="sec webkit">
                                        <div class="desc-head">
                                            <ul>
                                                <li><span class="color">Quand</span> <p>30 September 2013 | 11:30PM</p></li>
                                                <li><span class="color">Lieu</span> <p>Massive Club London, Leytonstone 13</p></li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </article>
                        </div>
                    </div>
                    <!-- Upcooming Event End -->
                </div>
                <!-- Span12 End -->
            </div>
            <!-- Row End -->
        </div>
        <!-- Container End -->
        <!-- Parallex Start -->
        <div class="parallex-count margin-bottom" data-speed="2" data-type="background">
            <!-- Container Start -->
            <div class="container">
                <div class="heading"><h2><span class="color">Prochain </span> Événement</h2></div>
                <div id="DateCountdown" class="date-count-down" data-date="2015-12-12 00:00:00"></div>
            </div>
            <!-- Container End -->
        </div>
        <!-- Parallex End -->
        <!-- Container Start -->
        <div class="container">
            <!-- Row Start -->
            <div class="row">
                <!-- Span12 Start -->
                <div class="span12">
                    <!-- Latest Album Start -->
                    <div class="latest-album">
                        <div class="heading"><h2><span class="color">Toutes les </span>Saisons</h2></div>
                        <div class="minus-margin">
                            <article>
                                <div class="text webkit">
                                    <div class="left-sec">
                                        <h3><a href="{{ path('eam_default_default_albums')}}">Photos</a></h3>
                                        <p>Les photos de Nicolas !</p>
                                    </div>
                                    <a class="icon-plus webkit" href="{{ path('eam_default_default_albums') }}"><i class="fa fa-plus fa-2x"></i></a>
                                </div>
                            </article>
                            <article>
                                <div class="text webkit">
                                    <div class="left-sec">
                                        <h3><a href="{{ path('eam_default_default_resultats')}}">Résultats</a></h3>
                                        <p>Tous les résultats</p>
                                    </div>
                                    <a class="icon-plus webkit" href="{{ path('eam_default_default_resultats')}}"><i class="fa fa-plus fa-2x"></i></a>
                                </div>
                            </article>
                            <article>
                                <div class="text webkit">
                                    <div class="left-sec">
                                        <h3><a href="{{ path('eam_default_default_records')}}">Records</a></h3>
                                        <p>Tous les records de Nicolas !</p>
                                    </div>
                                    <a class="icon-plus webkit" href="{{ path('eam_default_default_records')}}"><i class="fa fa-plus fa-2x"></i></a>
                                </div>
                            </article>
                        </div>
                    </div>
                    <!-- Latest Album End -->
                </div>
                <!-- Span12 End -->
            </div>
            <!-- Row End -->
        </div>
        <!-- Parallex Start -->
        <div class="parallex" data-speed="2" data-type="background">
            <div class="top-sec">
                <div class="heading"><h2><span class="color">Photo </span> Gallery</h2></div>
                <ul id="portfolio-filter">
                    <li><a href="#all" title="">Toutes</a></li>
                    <li><a href="#design" title="Business" data-rel="design">Entrainement</a></li>
                    <li><a href="#partner" title="Web Design" data-rel="partner">Compétition</a></li>
                    <li><a href="#political" title="Photography" data-rel="political">Manifestations</a></li>
                </ul>
            </div>
        </div>
        <!-- Parallex End -->
        <div class="clear"></div>
    </div>
    <!-- Content End -->
    {% include 'EAMDefaultBundle::footer.html.twig' %}
</div>
{% endblock %}